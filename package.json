{
  "name": "vue-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
